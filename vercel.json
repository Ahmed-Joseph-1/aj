{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": { "zeroConfig": true }
    },
    {
      "src": "ajax/static/ajax/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/ajax/static/ajax/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}
